<style>
    .allpage--body{
        background: url(my-account-bg.png) no-repeat top center;
    }
    .mediablock-equalized.mediablock-has-island{
        width: 100%;
        border-spacing: 5px;
    }
    .mediablock-has-island > div .island{
        margin-right: -5px;
    }
    .mediablock-has-island > div:first-child .island{
        margin-left: -5px;
        margin-right: 0;
    }
    @media screen and (max-width: 767px){
        .mediablock-has-island > div .island, 
        .mediablock-has-island > div:first-child .island{
            margin-left: -15px;
            margin-right: -15px;
        }
        .mediablock-has-island > div .island-top-square{
            border-radius: 4px;
        }
        .mediablock-has-island > div:first-child .island-top-square{
            border-radius: 0 0 4px 4px;
        }

    }
    .mediablock-has-island > .mediablock--primary,
    .mediablock-has-island > .mediablock--secondary{
        vertical-align: top;
    }
    .mediablock .island{
        padding: 30px;
        position: relative;
        margin-top: 5px;
        margin-bottom: 0;
    }
    .mediablock .island h2{
        margin-top: 0;
        color: #720000; /*@brand-primary-dark*/
        margin-bottom: 20px;
    }
    .mediablock .island h2 span{
        display: inline-block;
        line-height: .8;
    }
    .mediablock .island h2 .plate{
        margin-right: 10px;
    }
    .transactions-grid{
        margin-bottom: 10px;
    }
    .i--recent-transactions, .i--balance{
        fill: #fff;
        height: 45px;
    }
    .island-top-square{
        border-radius: 0 0 4px 4px;
    }
    .learn-more{
        font-size: 16px;    /*@brand-alt*/
        color: #006CD8;
    }
    .learn-more:hover{
        color: #1E90FF;     /*@brand-alt-bright*/
        text-decoration: none;
    }
    .island .learn-more{
        float: right;
        padding-top: 8px;
    }
    .transaction-type{
        color: #878787;
        font-size: 14px;
    }
</style>


    <section class="mediablock mediablock-equalized mediablock-3-to-2 mediablock-has-island">
