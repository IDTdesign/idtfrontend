<!DOCTYPE html>
<html class="no-js" lang="">
<head>
    <!-- Rendered with Eco -->
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="X-UA-TextLayoutMetrics" content="snap-vertical" />
    <meta http-equiv="X-UA-CSSOMElementFloatMetrics" content="false" />
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Site Properities -->
    <title><%= @site.title %></title>
    <meta name="description" content="<%= @site.description %>" />
    <meta name="keywords" content="<%= @site.keywords %>" />

    <!-- DocPad Meta -->
    <%- @getBlock('meta').toHTML() %>

    <!-- DocPad Styles + Our Own -->
    <%- @getBlock('styles').add(@getUrl(@site.styles)).add(@getUrl(@document.styles)).toHTML() %>

    <script src="<%= @getUrl('/vendor/modernizr/modernizr.min.js') %>"></script>
    <script src="<%= @getUrl('/icons/svg-icons.js') %>"></script>
</head>
<body>
    <div id="elements-placeholder" style="border: 0; clip: rect(0 0 0 0); overflow: hidden; margin: -1px; padding: 0; position: absolute; width: 1px; height: 1px;"></div>
    <script>document.getElementById("elements-placeholder").innerHTML = SVG_SVG_DEFS;</script>

    <nav class="offcanvas-menu navmenu navmenu-default navmenu-fixed-right" role="navigation">
        <ul class="nav navmenu-nav">
            <li><a href="#">About</a></li>
            <li><a href="#">Services</a></li>
            <li><a href="#">Rates</a></li>
            <li><a href="#">Support</a></li>
            <li><a href="#">My Account</a></li>
        </ul>
    </nav>

    <div class="allpage page-canvas">
        <header class="allpage--header">
            <div class="allpage--headerbg"></div>
            <div class="container">
                <div class="logo">
                    <a class="logo--link" href="#"><img src="<%= @getUrl('/images/logo-bossrevolution.png') %>" alt="BossRevolution"></a>
                </div>

                <nav class="navbar navbar-topline">
                    <p class="navbar-left navbar--text hidden-xs">
                        <a href="#">$1 FREE TRIAL <span class="hidden-sm">FOR NEW INTERNATIONAL CALLING CUSTOMERS</span></a>
                    </p>
                    <ul class="nav navbar-nav navbar--list navbar--list-position-right">
                       <li class="navbar--item dropdown">
                           <a href="#" class="navbar--link dropdown-toggle" data-toggle="dropdown">English <span class="caret"></span></a>
                           <ul class="navbar--submenu dropdown-menu" role="menu">
                               <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">English</a></li>
                               <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">Espanol</a></li>
                           </ul>
                       </li>
                       <li class="navbar--item dropdown">
                           <a href="#" class="navbar--link dropdown-toggle" data-toggle="dropdown">US <span class="caret"></span></a>
                           <ul class="navbar--submenu dropdown-menu" role="menu">
                               <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">US</a></li>
                               <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">UK</a></li>
                               <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">ES</a></li>
                               <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">DE</a></li>
                               <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">HK</a></li>
                               <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">SG</a></li>
                               <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">AU</a></li>
                           </ul>
                       </li>
                       <li class="navbar--item hidden-xs"><a href="#" class="navbar--link">Sign In</a></li>
                       <li class="navbar--item hidden-xs"><a href="#" class="navbar--link">Register</a></li>
                       <li class="navbar--item visible-xs">
                       <button type="button" class="navbar-toggle" data-toggle="offcanvas" data-recalc="false" data-target=".offcanvas-menu" data-canvas=".page-canvas">
                               <span class="">Menu</span>
                             </button>
                       </li>
                   </ul>
                </nav>

                <nav class="navbar navbar-bossmain hidden-xs" role="navigation">
                    <ul class="nav nav-pills navbar--list">
                        <li class="navbar--item"><a href="#" class="navbar--link">About</a></li>
                        <li class="navbar--item dropdown">
                            <a href="#" class="navbar--link pullin-space">Services</a><!--
                            --><button type="button" class="pullin-object dropdown-toggle" id="dropdownServices" data-toggle="dropdown">
                                <span class="btn btn-dropdown btn-xs">
                                    <span class="caret"></span>
                                </span>
                                <span class="sr-only">Toggle Dropdown</span>
                            </button>
                            <ul class="navbar--submenu dropdown-menu" role="menu" aria-labelledby="dropdownServices">
                                <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">International Calling</a></li>
                                <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">International Cellphone Recharges</a></li>
                                <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">Domestic Cellphone Recharges</a></li>
                                <li class="navbar--submenuitem dropdown-header">Additional Services</li>
                                <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">Money Transfer</a></li>
                                <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">Pre Paid Visa Card</a></li>
                                <li class="navbar--submenuitem"><a href="#" class="navbar--submenulink">Domestic Bill Payments</a></li>
                            </ul>
                        </li>
                        <li class="navbar--item"><a href="#" class="navbar--link">Rates</a></li>
                        <li class="navbar--item"><a href="#" class="navbar--link">Support</a></li>
                        <li class="navbar--item"><a href="#" class="navbar--link">My Account</a></li>
                    </ul>
                </nav>
            </div>
        </header>

        <div class="allpage--body">
            <div class="container">
                <%- @content %>
            </div>
        </div>

        <footer class="allpage--footer">
            <div class="container">

            </div>
        </footer>
    </div>

    <!-- DocPad Scripts + Our Own -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <%- @getBlock('scripts').add(@getUrl(@site.scripts)).add(@getUrl(@document.scripts)).toHTML() %>
</body>
</html>
